<p id="notice"><%= notice %></p>
 <div class="row">
   <div class="col-md-8">
     <ul class="breadcrumb">
     <li> <%= link_to 'Home', admin_dashboard_path %></li>
         <li class="active">Courses</li>
           
    </ul>
   </div>
   </div>
 <h1>Listing Courses</h1>
<div class="row">
  <div class="col-md-8">
 
     <table class="table table-bordered">
        <thead>
          <tr>
            <th>Title</th>
          </tr>
        </thead>

        <tbody>
          <% @courses.each do |course| %>
            <tr>
              <td><%= link_to course.title, admin_course_path(course.id) %></td>
             </tr>
          <% end %>
        </tbody>
      </table>
  </div>
  <div class="col-md-4"><%= link_to 'New Course', new_admin_course_path, :class=> "btn btn-success btn-large", :role=>"button" %></div>
</div>


