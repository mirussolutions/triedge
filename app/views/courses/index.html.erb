 <section id="about-us">
  <div class="container">
   <div class="row">
   <div class="col-md-12">
     <ul class="breadcrumb">
     <li> <%= link_to 'Home', root_path %></li>
         <li class="active">Courses</li>
           
    </ul>
   </div>
   </div>
  <div class="row">
  <div class="col-md-12">
      <div class="panel panel-primary">
        <div class="panel-heading">
        <h3 class="panel-title">Courses</h3>
        </div>
        <div class="panel-body"> <% @courses.each do |course| %>
           <%= link_to course.title, course_path(course.id), :class=> "btn btn-success btn-large listing" %>
                <% end %> </div>
      </div>
</div>
</div>
</div>
</section>


